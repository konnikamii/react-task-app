{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "prod": "set VITE_MODE=production && tsc --project tsconfig.server.json && node dist-server/server/server.js",
    "dev": "set VITE_MODE=development && concurrently \"tsc --project tsconfig.server.json --watch\" \"nodemon --trace-deprecation --config server/nodemon.json dist-server/server/server.js\"",
    "devs": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "clean": "powershell -Command \"Get-ChildItem -Path . -Filter 'vite.config.ts.timestamp-*' -Recurse | Remove-Item -Force\""
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "@tanstack/react-query": "^5.62.7",
    "@tanstack/react-router": "^1.87.7",
    "@tanstack/router-vite-plugin": "^1.95.5",
    "antd": "^5.23.1",
    "axios": "^1.7.9",
    "body-parser": "^1.20.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "d3": "^7.9.0",
    "dotenv": "^16.4.5",
    "express": "^4.21.2",
    "express-winston": "^4.2.0",
    "form-data": "^4.0.1",
    "html-minifier-terser": "^7.2.0",
    "multer": "^1.4.5-lts.1", 
    "react-cookie": "^7.2.0", 
    "react-ga4": "^2.1.0",
    "react-responsive": "^10.0.0",
    "winston": "^3.15.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.17.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.14.0",
    "postcss": "^8.5.1",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.18.2",
    "vite": "^6.0.5", 
    "@tailwindcss/typography": "^0.5.15",
    "@tanstack/eslint-plugin-query": "^5.59.1",
    "@tanstack/react-query-devtools": "^5.62.7",
    "@tanstack/router-devtools": "^1.87.7",
    "@tanstack/router-vite-plugin": "^1.87.7",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/d3": "^7.4.3",
    "@types/express": "^5.0.0",
    "@types/html-minifier-terser": "^7.0.2",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.7.4", 
    "@typescript-eslint/eslint-plugin": "^8.18.0",
    "@typescript-eslint/parser": "^8.18.0", 
    "concurrently": "^9.0.1", 
    "eslint-plugin-react": "^7.37.1", 
    "html-minifier-terser": "^7.2.0",
    "nodemon": "^3.1.7"
  }
}
